{
  "name": "headshot-aip-so",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:analyze": "vue-tsc && vite build --mode analyze",
    "preview": "vite preview",
    "preview:auto": "./run_auto_preview",
    "watch-env": "nodemon --watch .env --exec 'npm run build'",
    "preview:enhanced": "concurrently \"npm run watch-env\" \"./run_auto_preview\"",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint ."
  },
  "sideEffects": false,
  "dependencies": {
    "@capacitor/app": "7.0.2",
    "@capacitor/core": "7.4.3",
    "@capacitor/haptics": "7.0.2",
    "@capacitor/keyboard": "7.0.2",
    "@capacitor/status-bar": "7.0.2",
    "@ionic/vue": "8.7.3",
    "@ionic/vue-router": "8.7.3",
    "axios": "^1.11.0",
    "ionicons": "^7.0.0",
    "lucide-vue-next": "^0.474.0",
    "vue": "3.5.19",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.2",
    "terser": "^5.4.0",
    "typescript": "5.9.2",
    "vite": "5.4.19",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.1.10"
  },
  "overrides": {
    "rollup": "4.44.0"
  },
  "description": "An Ionic project"
}
